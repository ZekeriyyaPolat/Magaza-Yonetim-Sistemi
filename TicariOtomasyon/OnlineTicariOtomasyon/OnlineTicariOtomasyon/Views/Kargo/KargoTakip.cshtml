@using OnlineTicariOtomasyon.Models.Siniflar
@model List<KargoTakip>
@{
    ViewBag.Title = "KargoTakip";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<h2>KargoTakip</h2><br />
<table class="table table-striped">
    <tr>
        <th>Açıklama</th>
        <th>Tarih</th>

    </tr>
    @foreach (var X in Model)
    {
        <tr>
            <td>@X.Aciklama</td>
            <td>@X.TarihZaman</td>
        </tr>
    }
</table>
